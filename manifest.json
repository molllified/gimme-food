{
    "name": "Munchery",
    "description": "Munchery Tab - Where you get your food fix",
    "version": "1.0.2",
    "incognito": "split",
    "chrome_url_overrides": {
      "newtab": "index.html"
    },
    "permissions": [
      "http://*/",
      "https://*/",
      "background"
    ],
    "background": {
      "page": "background.html"
    },
    "content_security_policy": "script-src 'self' https://ssl.google-analytics.com; object-src 'self'",
    "manifest_version": 2
}
